                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH OrderedInvoices AS (
    SELECT
        c.CustomerId,
        tg.GenreId,
        il.Quantity,
        il.UnitPrice AS SumPrice,
        i.InvoiceDate,
        LAG(i.InvoiceDate) OVER (PARTITION BY c.CustomerId ORDER BY i.InvoiceDate) AS PreviousInvoiceDate,
        ROW_NUMBER() OVER (PARTITION BY c.CustomerId ORDER BY i.InvoiceDate) AS RowNum
    FROM
        InvoiceLine il
    INNER JOIN (
        SELECT 
            t.TrackId,
            t.GenreId
        FROM 
            Track t
        INNER JOIN 
            Genre g ON t.GenreId = g.GenreId
    ) AS tg ON il.TrackId = tg.TrackId
    INNER JOIN Invoice i ON il.InvoiceId = i.InvoiceId
    INNER JOIN Customer c ON i.CustomerId = c.CustomerId
)

SELECT
    CustomerId,
    GenreId,
    InvoiceDate,
    SUM(Quantity) AS Quantity,
    SUM(SumPrice) AS SumPrice,
    AVG(DATEDIFF(day, PreviousInvoiceDate, InvoiceDate)) AS NumberofDaysReturned
FROM
    OrderedInvoices
WHERE
    PreviousInvoiceDate IS NOT NULL
GROUP BY
    CustomerId, GenreId, InvoiceDate
ORDER BY
    Quantity DESC;</property>
